set(PACKAGE_VERSION "@RAINHDF_VERSION@")

# enforce major version match
if ("${PACKAGE_FIND_VERSION}" VERSION_GREATER @RAINHDF_VERSION@ OR NOT "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @RAINHDF_VERSION_MAJOR@)
  set(PACKAGE_VERSION_COMPATIBLE FALSE)
  set(PACKAGE_VERSION_EXACT FALSE)
else()
  set(PACKAGE_VERSION_COMPATIBLE TRUE)
  if ("${PACKAGE_FIND_VERSION}" VERSION_EQUAL @RAINHDF_VERSION@)
    set(PACKAGE_VERSION_EXACT TRUE)
  endif()
endif()
