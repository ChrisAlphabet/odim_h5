# Config file for the raincdf package
# Defines the following variables:
#  RAINHDF_INCLUDE_DIRS - include directories for rainhdf
#  RAINHDF_LIBRARIES    - libraries to link against

# compute paths
get_filename_component(RAINHDF_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(RAINHDF_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# out library dependencies
include("${RAINHDF_CMAKE_DIR}/rainhdf-targets.cmake")

# imported targets created by rainhdf-targets.cmake
set(RAINHDF_LIBRARIES rainhdf)

