set(PACKAGE_VERSION "@ODIM_H5_VERSION@")

# enforce major version match
if ("${PACKAGE_FIND_VERSION}" VERSION_GREATER @ODIM_H5_VERSION@ OR NOT "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @ODIM_H5_VERSION_MAJOR@)
  set(PACKAGE_VERSION_COMPATIBLE FALSE)
  set(PACKAGE_VERSION_EXACT FALSE)
else()
  set(PACKAGE_VERSION_COMPATIBLE TRUE)
  if ("${PACKAGE_FIND_VERSION}" VERSION_EQUAL @ODIM_H5_VERSION@)
    set(PACKAGE_VERSION_EXACT TRUE)
  endif()
endif()
